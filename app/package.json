{
  "name": "@report-portal/service-ui",
  "private": true,
  "version": "0.0.0",
  "license": "GPL-3.0-only",
  "scripts": {
    "eslint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "stylelint": "stylelint \"./**/*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "format:js": "npm run eslint -- --fix",
    "format:scss": "npm run stylelint -- --fix",
    "format": "npm run format:js && npm run format:scss && prettier --write */**/*.md",
    "dev": "webpack-dev-server --port 3000",
    "build": "webpack -p --env.production",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook_app",
    "test": "jest --notify",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:lang:en": "webpack --display none --env.dev && babel localization/translator.js | node",
    "manage:translations": "webpack --display none --env.dev && babel-node ./localization/translatorRunner.js",
    "manage:translations:test": "webpack --display none --env.dev && babel-node ./localization/translatorRunnerCheck.js",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "dependencies": {
    "array.find": "1.0.7",
    "array.findindex": "1.0.4",
    "axios": "0.17.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "c3": "0.6.7",
    "classnames": "2.2.5",
    "core-js": "2.5.7",
    "d3": "5.7.0",
    "d3-selection": "1.3.2",
    "dotenv": "5.0.1",
    "fast-deep-equal": "2.0.1",
    "fetch-jsonp": "1.1.3",
    "history": "4.7.2",
    "html-react-parser": "0.4.5",
    "intl": "1.2.5",
    "moment": "2.22.1",
    "perf-cascade": "2.5.4",
    "promise-polyfill": "8.1.0",
    "prop-types": "15.7.2",
    "pure-react-carousel": "1.19.0",
    "qhistory": "1.0.0",
    "qs": "6.5.2",
    "rc-slider": "8.6.3",
    "react": "16.6.0",
    "react-autosuggest": "9.3.4",
    "react-c3js": "0.1.20",
    "react-color": "2.17.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-custom-scrollbars": "4.2.1",
    "react-datepicker": "1.4.1",
    "react-dom": "16.6.0",
    "react-dropzone": "4.2.9",
    "react-event-observer": "0.5.11",
    "react-full-screen": "0.2.2",
    "react-ga": "2.5.3",
    "react-grid-layout": "0.16.6",
    "react-highlight": "0.12.0",
    "react-hot-loader": "3.1.1",
    "react-intl": "2.4.0",
    "react-popper": "1.3.3",
    "react-redux": "5.0.7",
    "react-select": "1.2.1",
    "react-tracking": "5.4.1",
    "react-transition-group": "2.3.1",
    "rebound": "0.1.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-first-router": "0.0.16-next",
    "redux-first-router-link": "1.4.2",
    "redux-form": "7.3.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "reset-css": "2.2.1",
    "semver-diff": "2.1.0",
    "simplemde": "1.11.2",
    "swagger-ui": "3.10.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.3",
    "@storybook/addon-options": "3.4.3",
    "@storybook/addon-viewport": "3.4.3",
    "@storybook/react": "3.4.3",
    "axios-mock-adapter": "1.15.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "0.28.11",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.3.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "2.22.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "lint-staged": "7.1.0",
    "node-sass": "4.9.0",
    "postcss-loader": "2.1.5",
    "prettier": "1.12.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.6.2",
    "react-intl-translations-manager": "5.0.3",
    "sass-loader": "6.0.7",
    "sass-resources-loader": "1.3.3",
    "storybook-addon-intl": "2.3.0",
    "storybook-host": "4.1.4",
    "storybook-readme": "3.3.0",
    "style-loader": "0.19.0",
    "stylelint": "9.9.0",
    "stylelint-config-recommended-scss": "3.2.0",
    "stylelint-declaration-use-variable": "1.7.0",
    "stylelint-prettier": "1.0.5",
    "stylelint-scss": "3.4.0",
    "svg-inline-loader": "0.8.0",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.7",
    "webpack-hot-middleware": "2.22.1",
    "webpack-notifier": "1.6.0",
    "why-did-you-update": "0.1.1"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(svg)$": "<rootDir>/test/__mocks__/svgMock.js",
      "^components[/](.+)": "<rootDir>/src/components/$1",
      "^controllers[/](.+)": "<rootDir>/src/controllers/$1",
      "^common[/](.+)": "<rootDir>/src/common/$1",
      "^pages[/](.+)": "<rootDir>/src/pages/$1",
      "^store[/](.+)": "<rootDir>/src/store/$1",
      "^routes[/](.+)": "<rootDir>/src/routes/$1",
      "^layouts[/](.+)": "<rootDir>/src/layouts/$1"
    },
    "notify": false
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  }
}

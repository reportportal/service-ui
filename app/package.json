{
  "name": "webpackbin-project",
  "version": "1.0.0",
  "description": "Project boilerplate",
  "scripts": {
    "eslint": "eslint -f node_modules/eslint-friendly-formatter \"src/**/*.jsx\" \"src/**/*.js\"",
    "dev": "webpack-dev-server --port 3000",
    "build": "webpack -p --env.production",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:bild": "build-storybook -c .storybook -o .storybook_app",
    "test": "jest --notify",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cerebral/forms": "^2.5.0",
    "@cerebral/http": "^3.6.0",
    "@cerebral/react": "^2.5.0",
    "@cerebral/router": "^2.5.0",
    "@cerebral/storage": "^3.4.0",
    "cerebral": "^3.5.0",
    "classnames": "^2.2.5",
    "fetch-jsonp": "^1.1.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "reset-css": "^2.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-options": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "storybook-host": "^4.1.4",
    "storybook-readme": "^3.1.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-notifier": "^1.5.0"
  },
  "author": "WebpackBin",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "notify": true
  }
}
